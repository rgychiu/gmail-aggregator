{
    "accounts": {
        "cred_rel_path": "accounts",
        "token_rel_path": "tokens",
        "flow_ext": ".pickle",
        "scopes": [
            "https://www.googleapis.com/auth/gmail.readonly",
            "https://www.googleapis.com/auth/gmail.send"
        ]
    },
    "api": {
        "user_id": "me",
        "unread_label": "UNREAD",
        "ext_labels": ["IMPORTANT"],
        "acct_payload": "messages",
        "message_labels": "labelIds",
        "message_data": "payload",
        "message_headers": "headers"
    },
    "template": {
        "single": "Date: {date}, From: {sender}, To: {recipient}, Subject: {subject}",
        "account": "Under inbox {name}:\n{body}",
        "aggregate": "Hello,\n\nHere is your configured email-aggregated digest for your inboxes:\n\n{body}\n\nThanks,\nGmail-Aggregator Team"
    }
}